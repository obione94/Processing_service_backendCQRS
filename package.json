{
  "name": "processing_backend",
  "version": "1.0.0",
  "description": "processing service  for handling  backend Command Service",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts,json --exec ts-node src/index.ts",
    "migrate:create": "cassandra-migrate create",
    "migrate:up": "cd ./src/scripts/migrations && cassandra-migrate up -k mon_keyspace"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "cassandra-driver": "^4.8.0",
    "cassandra-migrate": "^1.2.333",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "level": "^10.0.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=24"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leveldown": "^4.0.6",
    "@types/levelup": "^5.1.5",
    "@types/node": "^24.2.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
